{
  "name": "heneke/web-common-laravel",
  "description": "Common web request utilities for laravel",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/heneke/php-web-common"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "heneke/web-common": "^0.1.0",
    "illuminate/support": "^5.2",
    "symfony/psr-http-message-bridge": "^0.2.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.3.1",
    "phpunit/phpunit": "^5.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hendrik Heneke",
      "email": "heneke@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Heneke\\Web\\Common\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Heneke\\Web\\Common\\Laravel\\": "test/"
    }
  }
}
